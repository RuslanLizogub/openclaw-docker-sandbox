#############################################
# MANDATORY: LM Studio local server config
#############################################
# Keep provider=openai and point to local OpenAI-compatible endpoint.
AI_PROVIDER=openai
OPENAI_API_KEY=lm-studio-local-access
OPENAI_BASE_URL=http://host.docker.internal:1234/v1

# IMPORTANT:
# Use openai/<LM_STUDIO_MODEL_ID> so OpenClaw keeps provider=openai.
# Run: curl http://localhost:1234/v1/models
# and copy "id" exactly from the response.
OPENAI_MODEL=openai/google/gemma-3-12b
# Examples:
# OPENAI_MODEL=openai/openai/gpt-oss-20b
# OPENAI_MODEL=openai/google/gemma-3-4b

# Required for local/custom model ids that are not in OpenClaw built-in catalog.
OPENAI_CUSTOM_MODEL_ENABLED=true
OPENAI_CUSTOM_MODEL_API=openai-completions

# Recommended: always sync model from .env to avoid stale openclaw.json model config.
OPENCLAW_SYNC_ENV_MODEL=true

# Leave non-selected provider keys empty
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=

# Optional Telegram integration
TELEGRAM_BOT_TOKEN=
TELEGRAM_ALLOWED_USER_IDS=

#############################################
# DEFAULTS: safe for local LM Studio mode
#############################################
# Build Version Pins (validated 2026-02-16)
NODE_VERSION=22.22.0
OPENCLAW_REF=v2026.2.15
PLAYWRIGHT_IMAGE_TAG=v1.58.2-noble

OPENAI_TEMPERATURE=0.7
ANTHROPIC_MODEL=claude-sonnet-4-5
ANTHROPIC_TEMPERATURE=0.7
GOOGLE_GENERATIVE_AI_MODEL=gemini-3-pro-preview
GOOGLE_GENERATIVE_AI_TEMPERATURE=0.7

# Agent defaults
AGENT_NAME=Claw
SOUL_FILE=default.soul.md
CONFIRMATION_MODE=full-auto
SANDBOX_FORCE_CHANNEL_FULL_AUTO=true
SANDBOX_DISABLE_WORKSPACE_TEMPLATES=true
SANDBOX_DISABLE_BOOTSTRAP_PROMPTS=true

# Runtime path defaults inside container
OPENCLAW_STATE_DIR=/home/openclaw/.openclaw
OPENCLAW_CONFIG_PATH=/home/openclaw/.openclaw/openclaw.json
# Use state volume for temp files to avoid /tmp ENOSPC in Docker VM.
OPENCLAW_TMPDIR=/home/openclaw/.openclaw/tmp
# Optional fixed gateway token; leave empty to auto-generate once and persist in config.
OPENCLAW_GATEWAY_TOKEN=
BROWSER_DEFAULT_PROFILE=openclaw
BROWSER_HEADLESS=true
BROWSER_NO_SANDBOX=true
BROWSER_EXECUTABLE_PATH=
BROWSER_PROFILE_PATH=/home/openclaw/.openclaw/browser/default/user-data
WORKSPACE_PATH=/home/openclaw/.openclaw/workspace

# Optional integrations defaults
DISCORD_BOT_TOKEN=
DISCORD_ALLOWED_USER_IDS=

# Security/runtime defaults
NODE_ENV=production

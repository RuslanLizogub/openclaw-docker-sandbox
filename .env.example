#############################################
# MANDATORY: fill before first `docker compose up --build`
#############################################
# Required: choose one provider
AI_PROVIDER=openai

# Required: fill key for the selected provider above
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=
# Backward-compatible alias (optional): if GEMINI_API_KEY is empty,
# entrypoint maps GOOGLE_GENERATIVE_AI_API_KEY -> GEMINI_API_KEY
GOOGLE_GENERATIVE_AI_API_KEY=

# Required only if you enable Telegram integration
TELEGRAM_BOT_TOKEN=
TELEGRAM_ALLOWED_USER_IDS=

#############################################
# DEFAULTS: safe to keep as-is
#############################################
# Build Version Pins (validated 2026-02-16)
NODE_VERSION=22.22.0
OPENCLAW_REF=v2026.2.15
PLAYWRIGHT_IMAGE_TAG=v1.58.2-noble

# Provider settings defaults
OPENAI_MODEL=gpt-5.1-codex
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_TEMPERATURE=0.7

ANTHROPIC_MODEL=claude-sonnet-4-5
ANTHROPIC_TEMPERATURE=0.7

GOOGLE_GENERATIVE_AI_MODEL=gemini-3-pro-preview
GOOGLE_GENERATIVE_AI_TEMPERATURE=0.7

# Agent defaults
AGENT_NAME=Claw
SOUL_FILE=default.soul.md
# Modes: suggest | auto-edit | full-auto
CONFIRMATION_MODE=suggest

# Runtime path defaults inside container
OPENCLAW_STATE_DIR=/home/openclaw/.openclaw
OPENCLAW_CONFIG_PATH=/home/openclaw/.openclaw/openclaw.json
BROWSER_DEFAULT_PROFILE=openclaw
BROWSER_HEADLESS=true
BROWSER_NO_SANDBOX=true
# Optional override; in Docker this is autodetected from /ms-playwright
BROWSER_EXECUTABLE_PATH=
BROWSER_PROFILE_PATH=/home/openclaw/.openclaw/browser/default/user-data
WORKSPACE_PATH=/home/openclaw/.openclaw/workspace

# Optional integrations defaults
DISCORD_BOT_TOKEN=
DISCORD_ALLOWED_USER_IDS=

# Security/runtime defaults
NODE_ENV=production
